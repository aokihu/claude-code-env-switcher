{
  "name": "router-switch",
  "version": "1.0.0",
  "description": "A CLI tool to switch between AI providers and models using environment variables",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "router-switch": "dist/index.js"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outfile ./dist/rsc --target bun --compile",
    "dev": "bun --watch ./src/index.ts",
    "start": "bun ./dist/index.js",
    "test": "bun test"
  },
  "keywords": [
    "cli",
    "ai",
    "providers",
    "models",
    "environment-variables"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "dependencies": {
    "commander": "^12.1.0"
  }
}
